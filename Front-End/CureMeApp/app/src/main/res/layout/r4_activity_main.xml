<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".R4_MainActivity">

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="491dp"
        android:layout_marginTop="240dp"
        android:orientation="horizontal"
        app:layout_constraintTop_toTopOf="parent"
        tools:layout_editor_absoluteX="0dp"
        tools:ignore="MissingConstraints">

        <LinearLayout
            android:id="@+id/linearLayout"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:gravity="center"
            android:orientation="vertical"
            android:weightSum="100"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="0.454"
            app:layout_constraintStart_toStartOf="parent"
            tools:layout_editor_absoluteY="266dp">


            <ListView
                android:id="@+id/R4_listViewId"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_margin="25dp"
                android:layout_weight="70"
                android:divider="@color/black"
                android:dividerHeight="2dp"
                android:listSelector="@color/md_theme_light_primaryContainer"
                app:layout_constraintTop_toBottomOf="@+id/curemeapp"
                tools:layout_editor_absoluteX="5dp" />

        </LinearLayout>

    </LinearLayout>

    <TextView
        android:id="@+id/R4_nameText"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentTop="true"
        android:layout_alignParentBottom="true"

        android:layout_marginStart="3dp"
        android:layout_marginTop="24dp"
        android:gravity="top"
        android:text=" Γιωργος \n Παπαδόπουλος"
        android:textColor="#000000"
        android:textSize="15sp"
        app:layout_constraintStart_toEndOf="@+id/group_3081"
        app:layout_constraintTop_toBottomOf="@+id/historyText"
        tools:ignore="MissingConstraints" />

    <TextView
        android:id="@+id/historyText"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentLeft="true"
        android:layout_alignParentTop="true"
        android:layout_alignParentRight="true"
        android:layout_alignParentBottom="true"
        android:layout_marginStart="16dp"
        android:layout_marginTop="91dp"
        android:drawableLeft="@drawable/time"
        android:gravity="top"
        android:text="Ιστορικό Ασθενή"
        android:textColor="#000000"
        android:textSize="15sp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        tools:ignore="MissingConstraints" />




    <RelativeLayout
        android:id="@+id/group_3081"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="32dp"
        android:layout_marginTop="119dp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        tools:ignore="MissingConstraints">

        <View
            android:id="@+id/ellipse_252"
            android:layout_width="60.91dp"
            android:layout_height="56.63dp"
            android:layout_marginStart="2.97dp"
            android:layout_marginTop="3.3dp"
            android:background="@drawable/ellipse_252_shape" />


        <RelativeLayout
            android:id="@+id/group_2490"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginStart="0dp"
            android:layout_marginTop="0dp">


            <RelativeLayout
                android:id="@+id/group_2489"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginStart="0dp"
                android:layout_marginTop="0dp">


                <RelativeLayout
                    android:id="@+id/group_2488"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginStart="0dp"
                    android:layout_marginTop="0dp">


                    <RelativeLayout
                        android:id="@+id/group_2487"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_marginStart="0dp"
                        android:layout_marginTop="0dp">


                        <RelativeLayout
                            android:id="@+id/group_2486"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_marginStart="0dp"
                            android:layout_marginTop="0dp">


                            <RelativeLayout
                                android:id="@+id/group_2485"
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:layout_marginStart="0dp"
                                android:layout_marginTop="0dp">


                                <RelativeLayout
                                    android:id="@+id/group_2484"
                                    android:layout_width="wrap_content"
                                    android:layout_height="wrap_content"
                                    android:layout_marginStart="0dp"
                                    android:layout_marginTop="0dp">

                                    <ImageView
                                        android:id="@+id/path_610"
                                        android:layout_width="67dp"
                                        android:layout_height="60dp"
                                        android:layout_marginStart="0dp"
                                        android:layout_marginTop="0dp"
                                        android:contentDescription="path_610"
                                        android:src="@drawable/path_610" />

                                </RelativeLayout>

                            </RelativeLayout>

                        </RelativeLayout>

                        <ImageView
                            android:id="@+id/path_611"
                            android:layout_width="44.48dp"
                            android:layout_height="22.32dp"
                            android:layout_marginStart="12dp"
                            android:layout_marginTop="38dp"
                            android:contentDescription="path_611"
                            android:src="@drawable/path_611" />

                        <ImageView
                            android:id="@+id/path_612"
                            android:layout_width="26.8dp"
                            android:layout_height="24dp"
                            android:layout_marginStart="20dp"
                            android:layout_marginTop="15dp"
                            android:contentDescription="path_612"
                            android:src="@drawable/path_612" />

                    </RelativeLayout>

                </RelativeLayout>

            </RelativeLayout>

        </RelativeLayout>

    </RelativeLayout>
</androidx.constraintlayout.widget.ConstraintLayout>